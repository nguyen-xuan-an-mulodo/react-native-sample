{
  "name": "BReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "appium": "appium",
    "xcodebuild": "xcodebuild -workspace ios/BReactNative.xcworkspace -scheme BReactNative -derivedDataPath ios/build -destination \"platform=iOS Simulator,name=iPhone 6\"",
    "test": "jest",
    "test-and-shutdown-ios-simulator": "yarn test && xcrun simctl shutdown booted"
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.4"
  },
  "devDependencies": {
    "appium": "^1.9.0",
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "4.0.0",
    "jest": "23.5.0",
    "react-test-renderer": "16.3.1",
    "wd": "^1.10.3",
    "resemblejs": "^2.10.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
